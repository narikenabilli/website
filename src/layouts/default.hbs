<!DOCTYPE HTML>
<html lang="en" itemscope itemtype="http://schema.org/Article">
<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="author" content="Maurizio Casimirri (http://github.com/mcasimir)">
  <title>{{ $current.title }}</title>
  {{#if $current.description }}<meta name="description" content="{{ $current.description }}" />{{/if}}
  {{#if $current.keywords }}<meta name="description" content="{{ $current.keywords }}" />{{/if}}
  {{#each stylesheets.items}}<link href="{{$path}}" rel="stylesheet" />{{/each}}
   <link rel="shortcut icon" href="/stacktic/assets/img/favicon.png" />
  <!--[if lt IE 9]>
    <script src="//oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="//oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
  <![endif]-->
</head>
<body>

{{>'nav'}}

{{{ yield 'hero' }}}

  <div class="container">

{{#if $current.toc}}
<div class="row">
  <div class="col-sm-3 sidebar-container">
    <ul class="toc sidebar nav" id="toc">
    </ul>
  </div>
  <div class="col-sm-9">
<div class="content">
{{#unless isHome}}
<div class="page-header"><h1>{{$current.title}}</h1></div>
{{/unless}}

{{{ yield }}}  
</div>
  </div>
</div>
{{else}}
<div class="content">
{{#unless isHome}}
<div class="page-header"><h1>{{$current.title}}</h1></div>
{{/unless}}

{{{ yield }}}  
</div>
{{/if}}
  </div>
{{> 'footer' }}

<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-48036416-2', 'auto');
  ga('send', 'pageview');

</script>

{{#each scripts.items}}
  <script src="{{$path}}"></script>
{{/each}}

</body>
</html>